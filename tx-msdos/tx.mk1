!define BLANK ""
INSTRUCTION_SET=-2 # 80286
OPTIMIZATION=-osbhlsp

disk.obj : src\disk.c .AUTODEPEND
 *wcc src\disk.c -i="$(%watcom)/h" $(INSTRUCTION_SET) $(OPTIMIZATION) -w4 -e10 -d1 -bt=dos -fo=.obj -ms

